#include<stdio.h>
#include<math.h>
int main() 
{
    float a,b,c;
    printf("Enter the values of a,b,c :\n");
    scanf("%f%f%f",&a,&b,&c);
    float disc=b*b-a*c*4;
    float root1=(-b+sqrt(disc))/(2*a);
    float root2=(-b-sqrt(disc))/(2*a);
    printf("The roots are:\nroot1 = %0.1f\nroot2 = %0.1f",root1,root2);
    return 0;
    
}
